{
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "no-console": 0,
    "no-unused-vars": 0,
    "no-loop-func": 0,
    "no-throw-literal": 0,
    "no-underscore-dangle": 0,
    "no-return-await": 0,
    "arrow-body-style": 0,
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "array-callback-return": 0,
    "prefer-const": 0,
    "no-await-in-loop": 0,
    "import/no-extraneous-dependencies": 0,
    "camelcase": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "operator-assignment": 0,
    "max-len": 0,
  }
}